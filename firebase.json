{
  "hosting": {
    "public": "dist/client",
    "rewrites": [
      {
        "source": "**/**",
        "function": "ReactApp"
      }
    ]
  },
  "functions": {
    "runtime": "nodejs10",
    "source": ".",
    "ignore": [
      ".firebase/**",
      ".firebaserc",
      "firebase.json",
      "**/node_modules/**",
      "**/public/**",
      "**/src/**",
      "**/.cache/**"
    ]
  }
}
